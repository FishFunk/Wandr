#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@google-cloud/firestore/node_modules/google-auth-library/node_modules/semver/bin/semver" "$@"
  ret=$?
else 
  node  "$basedir/../@google-cloud/firestore/node_modules/google-auth-library/node_modules/semver/bin/semver" "$@"
  ret=$?
fi
exit $ret
